package main

import "fmt"

func main (){
    var idade int
    
    fmt.Println("Digite sua idade:")
    fmt.Scan(&idade)
    
    if idade < 0 {
        fmt.Println("Idade invalida. Digite uma idade maior que 0")
        return
    }
    
    if idade < 16 {
        fmt.Println("NÃ£o-eleitor")
    } else if idade >= 18 && idade <= 65 {
        fmt.Println("Eleitor obrigatÃ³rio")
    } else {
        fmt.Println("Eleitor facultativo")
    }
}
