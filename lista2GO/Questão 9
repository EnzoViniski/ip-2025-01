package main

import "fmt"

func main (){
	var n1 float64

	fmt.Println("Digite o valor da compra")
	fmt.Scan(&n1)

	if n1 < 10 {
		multi := n1 * 1.7
		fmt.Println("O valor da venda ser치" , multi)
	} else if n1 >= 10 && n1 < 30 {
		multi := n1 * 1.5
		fmt.Println("O valor da venda ser치" , multi)
	} else if n1 >= 30 && n1 < 50 {
		multi := n1 * 1.4
		fmt.Println("O valor da venda ser치" , multi)
	} else if n1 >= 50 {
		multi := n1 * 1.3
		fmt.Println("O valor da venda ser치" , multi)
	}
}
