package main

import "fmt"

func main (){
	lista := []int{5, 4 ,3 ,2 ,10}

	x := 10 

	encontrado := -1

	for i, numero := range lista {
		if numero == x {
			encontrado = i
			break
		}
	}

	if encontrado !=  -1 {
		fmt.Printf("Encontrei o número %d. Ele está na posição %d da fila.", x, encontrado)
	} else {
		fmt.Printf("Não encontrei o número %d na lista...", x)
	}
}
